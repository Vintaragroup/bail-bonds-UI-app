# Frontend Firebase config (values from Firebase Console Web App settings)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_APP_ID=
# Optional extras
# VITE_FIREBASE_MEASUREMENT_ID=
# VITE_FIREBASE_MESSAGING_SENDER_ID=
# VITE_ENABLE_AUTH_PREVIEW=true

# Stripe publishable key (test or live depending on environment)
VITE_STRIPE_PUBLISHABLE_KEY=

# --- API / Compose variables ---
# Mongo connection (Atlas recommended)
MONGO_URI=
MONGO_DB=warrantdb

# CORS origins for the API when NODE_ENV=production (comma-separated)
# In dev, http://localhost:5173 is allowed by default
WEB_ORIGIN=http://localhost:5173

# SPA build-time API base (optional; runtime public/env.js is preferred)
VITE_API_URL=http://localhost:8080
